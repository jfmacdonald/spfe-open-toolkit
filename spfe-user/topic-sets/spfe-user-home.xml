<topic-set   xmlns="http://spfeopentoolkit.org/ns/spfe-ot/config"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://spfeopentoolkit.org/ns/spfe-ot/config http://spfeopentoolkit.org/spfe-ot/1.0/schemas/config/spfe-config.xsd">
        <topic-set-id>spfe-user-home</topic-set-id>
    <title>SPFE User Docs</title>
    <topic-set-type>guide</topic-set-type>
        <topic-types>

            <topic-type>         
                <name>{http://spfeopentoolkit.org/ns/spfe-docs}content-set-home-topic</name>
                <href>../../spfe-docs/topic-types/content-set-home-topic/config.xml</href>
            </topic-type>

        </topic-types>
        <presentation-types>
            <presentation-type>eppo</presentation-type>
        </presentation-types>        
        <output-formats>
            <output-format>
                <name>xhtml</name>
            </output-format>
        </output-formats>
        

        <sources>
            <authored-content>
                <include>../content/text/home/*.xml</include>
                <include>../content/text/reference/*.xml</include>
            </authored-content>
        </sources>

        <default-topic-scope>spfe</default-topic-scope>
        <default-subject-scope>spfe</default-subject-scope>
        <strings>
            <string id="eppo-simple-topic-set-title">Using SPFE Open Toolkit</string>
        </strings>
    </topic-set>

