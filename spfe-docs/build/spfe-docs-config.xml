<spfe xmlns="http://spfeopentoolkit.org/spfe-ot/1.0/schemas/spfe-config"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://spfeopentoolkit.org/spfe-ot/1.0/schemas/spfe-config http://spfeopentoolkit.org/spfe-ot/1.0/schemas/config/spfe-config.xsd">
    <doc-set>
        <doc-set-id>spfe-docs</doc-set-id>
        <title>SPFE Documentation</title>
        <topic-set-type-order>
            <topic-set-type>docset</topic-set-type>
            <topic-set-type>guide</topic-set-type>
            <topic-set-type>task</topic-set-type>
            <topic-set-type>reference</topic-set-type>
            <topic-set-type>list</topic-set-type>
        </topic-set-type-order>
        <home-topic-set>spfe-docs-docset</home-topic-set>
        <topic-sets>
            
            <topic-set>
                <topic-set-id>spfe-development</topic-set-id>
                <href>topic-sets/spfe-development.xml</href>
            </topic-set>    
            
            <topic-set>
                <topic-set-id>spfe-docs-docset</topic-set-id>
                <href>topic-sets/spfe-docs-docset.xml</href>
            </topic-set>
            
            <topic-set>
                <topic-set-id>spfe-function-reference</topic-set-id>
                <href>topic-sets/spfe-function-reference.xml</href>
            </topic-set>
            
            <topic-set>
                <topic-set-id>spfe-configuration-reference</topic-set-id>
                <href>topic-sets/spfe-configuration-reference.xml</href>
            </topic-set>
            
            <topic-set>
                <topic-set-id>spfe-docs-lists</topic-set-id>
                <href>topic-sets/spfe-docs-lists.xml</href>
            </topic-set>
        </topic-sets>
        
        <subject-types>
            <subject-type>
                <id>config-setting</id>
                <aliases>
                    <singular>Configuration setting</singular>
                    <plural>Configuration settings</plural>
                </aliases>
            </subject-type>
        </subject-types>

        <style>
            <html-style-directories>
                <include>${SPFEOT_HOME}/plugins/eppo-simple/output-types/html/css</include>
            </html-style-directories>
        </style>
        <messages>warning info</messages>
        

        
        <output-formats>
            <output-format>
                <name>html</name>
                <href>../../spfe-ot/plugins/eppo-simple/output-types/html/config.xml</href>
            </output-format>
        </output-formats>
        
        <wip-site>
            <url>wip-site.href</url>
            <ftp>
                <url>wip-site.ftp.url</url>
                <user>wip-site.ftp.user</user>
                <password>wip-site.ftp.password</password>
            </ftp>
            <path>wip-site.ftp</path>
        </wip-site>

        <strings>
            <string id="eppo-simple-topic-set-product">SPFE Open Toolkit</string>
            <string id="eppo-simple-topic-set-release">1.0 (alpha 1)</string>
            <string id="eppo-simple-topic-set-copyright">Copyright Â© 2013 Analecta Communications Inc.</string>
        </strings>
     </doc-set>
    


    

</spfe>