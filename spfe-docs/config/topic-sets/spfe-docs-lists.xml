<topic-set xmlns="http://spfeopentoolkit.org/ns/spfe-ot/config"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://spfeopentoolkit.org/ns/spfe-ot/config http://spfeopentoolkit.org/spfe-ot/1.0/schemas/config/spfe-config.xsd">
    <topic-set-id>spfe-docs-lists</topic-set-id>
    <title>Lists of Topics by Subject</title>
    <topic-set-type>list</topic-set-type>
    <topic-set-link-priority>0</topic-set-link-priority>

    <topic-types>
        <topic-type>
            <name>{http://spfeopentoolkit.org/ns/spfe-docs}subject-topic-list</name>
            <href>../../../spfe-docs/topic-types/topic-subject-list-topic/config.xml</href>
        </topic-type>
    </topic-types>
    <presentation-types>
        <presentation-type>eppo</presentation-type>
        <!--<presentation-type>dita</presentation-type>-->
    </presentation-types>
    <output-formats>
        <output-format>
            <name>xhtml</name>
        </output-format>
    </output-formats>
    
    <sources>
        <sources-to-extract-content-from>
            <files><include>${CONTENT_SET_BUILD_DIR}/catalogs/*.catalog.xml</include></files>
        </sources-to-extract-content-from>
    </sources>

    

    <default-topic-scope>spfe</default-topic-scope>
    <default-subject-scope>spfe</default-subject-scope>
</topic-set>

