<topic-set xmlns="http://spfeopentoolkit.org/ns/spfe-ot/config"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://spfeopentoolkit.org/ns/spfe-ot/config http://spfeopentoolkit.org/spfe-ot/1.0/schemas/config/spfe-config.xsd">
    <topic-set-id>spfe-demos</topic-set-id>
    <title>EPPO-simple Demos</title>
    <topic-set-type>guide</topic-set-type>

    <topic-types>
        <topic-type>
            <name>{http://spfeopentoolkit.org/ns/spfe-docs}generic-topic</name>
            <href>../../topic-types/generic-topic/config.xml</href>
        </topic-type>
    </topic-types>

    <presentation-types>
        <presentation-type>eppo</presentation-type>
        <!--            <presentation-type>dita</presentation-type>
-->
    </presentation-types>
    <output-formats>
        <output-format>
            <name>xhtml</name>
        </output-format>
    </output-formats>

    <sources>
        <authored-content>
            <files>
                <include>../../content/topics/demos/*.xml</include>
                <include>../../content/topics/fragments/*.xml</include>
            </files>
        </authored-content>
    </sources>

    <default-topic-scope>spfe</default-topic-scope>
    <default-subject-scope>spfe</default-subject-scope>
</topic-set>
