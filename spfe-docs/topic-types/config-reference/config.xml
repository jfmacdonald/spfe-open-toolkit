<?xml version="1.0" encoding="UTF-8"?>
<spfe xmlns="http://spfeopentoolkit.org/spfe-ot/1.0/schemas/spfe-config"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://spfeopentoolkit.org/spfe-ot/1.0/schemas/spfe-config http://spfeopentoolkit.org/spfe-ot/1.0/schemas/config/spfe-config.xsd">
    <topic-type>
        <xmlns>http://spfeopentoolkit.org/spfe-docs/topic-types/config-reference</xmlns>
        <aliases>
            <singular>Configuration setting</singular>
            <plural>Configuration settings</plural>
        </aliases>
        <scripts>
            <extract>
                <script>../../topic-types/config-reference/scripts/extract/schema-defs.xsl</script>
                <script>${SPFEOT_HOME}/1.0/scripts/common/utility-functions.xsl</script>
            </extract>
            <synthesis>
                <script>../../topic-types/config-reference/scripts/synthesis/synthesize-config-ref.xsl</script>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/synthesis/synthesize-text-structures.xsl</script>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/synthesis/subject-affinities.xsl</script>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/synthesis/synthesize-strings.xsl</script>
                <script>../../topic-types/shared/scripts/synthesis/subject-affinities.xsl</script>
                <script>${SPFEOT_HOME}/1.0/scripts/common/utility-functions.xsl</script>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/object-types/graphics/scripts/synthesis/synthesis-fig.xsl</script>
            </synthesis>
            <link-catalog>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/link-catalog/topic/topic-link-catalog.xsl</script>
                <script>${SPFEOT_HOME}/1.0/scripts/common/utility-functions.xsl</script>
            </link-catalog>
            <toc>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/toc/toc.xsl</script>
                <script>${SPFEOT_HOME}/1.0/scripts/common/utility-functions.xsl</script>
                <script>../../topic-types/config-reference/scripts/toc/schema-toc.xsl</script>
            </toc>
            <image-list>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/presentation/get-image-list.xsl</script>
                <script>${SPFEOT_HOME}/1.0/scripts/common/utility-functions.xsl</script>
            </image-list>
            <presentation-web>
                <script>../../topic-types/config-reference/scripts/presentation/present-config-ref.xsl</script>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/presentation/present-topic-set-toc.xsl</script>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/presentation/present-references.xsl</script>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/presentation/present-text-structures.xsl</script>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/presentation/present-header-and-footer.xsl</script>
                <script>${SPFEOT_HOME}/plugins/eppo-simple/topic-types/shared/scripts/presentation/present-topic-set.xsl</script>
                <script>${SPFEOT_HOME}/1.0/scripts/common/utility-functions.xsl</script>
            </presentation-web>
        </scripts>
